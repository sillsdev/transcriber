{
  "name": "sil-transcriber",
  "version": "0.6.2",
  "description": "transcribe audio files for Paratext or Elan",
  "private": true,
  "dependencies": {
    "@devexpress/dx-react-chart": "^2.4.0",
    "@devexpress/dx-react-chart-material-ui": "^2.4.0",
    "@devexpress/dx-react-core": "^2.4.0",
    "@devexpress/dx-react-grid": "^2.4.0",
    "@devexpress/dx-react-grid-material-ui": "^2.4.0",
    "@dr-kobros/react-webfont-loader": "^8.0.0",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@orbit/coordinator": "^0.16.4",
    "@orbit/core": "^0.16.3",
    "@orbit/data": "^0.16.4",
    "@orbit/indexeddb": "^0.16.4",
    "@orbit/indexeddb-bucket": "^0.16.3",
    "@orbit/jsonapi": "^0.16.4",
    "@orbit/memory": "^0.16.4",
    "adm-zip": "^0.4.13",
    "asar": "^2.0.3",
    "auth0-js": "^9.12.2",
    "aws-amplify": "^2.2.2",
    "axios": "^0.19.2",
    "electron-is-dev": "^1.1.0",
    "fs-extra": "^8.1.0",
    "js-base64": "^2.5.1",
    "jwt-decode": "^2.2.0",
    "keycode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "node-html-encoder": "0.0.2",
    "original-fs": "^1.1.0",
    "react": "^16.12.0",
    "react-datasheet": "^1.4.0",
    "react-dom": "^16.12.0",
    "react-file-drop": "^0.2.8",
    "react-hot-loader": "^4.12.19",
    "react-icons": "^3.9.0",
    "react-localization": "^1.0.15",
    "react-orbitjs": "^0.1.18",
    "react-player": "^1.14.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-select": "^3.0.8",
    "react-string-replace": "^0.4.4",
    "reactn": "^2.2.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "clean": "node noHomepage.js && rimraf build dist converage",
    "start": "react-scripts start",
    "build": "npm run clean && react-scripts build",
    "test": "craco test",
    "eject": "craco eject",
    "dist": "electron-builder",
    "electron-dev-mac": "concurrently \"BROWSER=none craco start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev": "node edPackage.js -dev && env-cmd -f .env.electron concurrently \"craco start \" \"wait-on http://localhost:3000 && electron .\"",
    "electron-debug": "node edPackage.js -dev && env-cmd -f .env.electron craco start",
    "electron-prod": "node edPackage.js && set NODE_ENV=production && env-cmd -f .env.electron concurrently \"npm run start \" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "electron-pack": "npm run stamp && node edPackage.js && env-cmd -f .env.electron craco build -w --config craco.config.electron.js",
    "stamp": "node writeDate.js",
    "deploy": "npm run stamp && amplify publish",
    "deploySetup": "amplify init && git checkout amplify && git checkout .gitignore",
    "appdev": "amplify env checkout appdev && node changeEnv.js appdev && git checkout amplify/*",
    "appqa": "amplify env checkout appqa && node changeEnv.js appqa && git checkout amplify/*",
    "appprod": "amplify env checkout appprod && node changeEnv.js appprod && git checkout amplify/*",
    "dev": "amplify env checkout dev && node changeEnv.js dev && git checkout amplify/*",
    "qa": "amplify env checkout qa && node changeEnv.js qa && git checkout amplify/*",
    "prod": "amplify env checkout prod && node changeEnv.js prod && git checkout amplify/*"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-amplify/pubsub": "^1.2.4",
    "@craco/craco": "^5.6.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/adm-zip": "^0.4.32",
    "@types/auth0-js": "^9.12.3",
    "@types/jest": "^24.9.1",
    "@types/js-base64": "^2.3.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.12.26",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.10",
    "concurrently": "^5.1.0",
    "craco-plugin-react-hot-reload": "^0.1.0",
    "electron": "^7.1.11",
    "electron-builder": "^22.3.2",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-react-devtools": "^0.5.3",
    "env-cmd": "^10.0.1",
    "npm-watch": "^0.6.0",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.1",
    "source-map-explorer": "^2.2.2",
    "wait-on": "^4.0.0",
    "write": "^2.0.0"
  },
  "author": {
    "name": "SIL International",
    "email": "info@sil.org",
    "url": "http://sil.org"
  },
  "build": {
    "appId": "sil.lsdev.sil-transcriber-electron",
    "productName": "SIL Transcriber Desktop Extension",
    "copyright": "Copyright Â© 2020 ${author}",
    "win": {
      "icon": "public/favicon.bmp"
    },
    "linux": {
      "icon": "public/favicon.png",
      "target": [
        "deb",
        "snap"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "directories": {
      "buildResources": "assets"
    },
    "extraFiles": [
      {
        "from": "public/localization",
        "to": "localization",
        "filter": "*.json"
      }
    ]
  }
}
